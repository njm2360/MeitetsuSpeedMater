      else
      {
        shiftsenro = 0;
      }

      buf.pushImage(70 + shiftsenro, 26, senroWidth, senroHeight, senro);
      buf.fillRect(0, 26, 84, 10, 0x000000U);
      buf.fillRect(265, 0, 120, 48, 0x000000U);
      drawflag=1;
    }
    if (drawflag)
    {
      drawflag = 0;
      lastsp = sp;
      lcd_buffer_write();
    }
  }
}

inline void lcd_buffer_write()
{
  lcd.setAddrWindow(0, 0, 320, 96);
  lcd.pushPixels((lgfx::swap565_t *)buf.getBuffer(), 640 * 48);
}
